{
  "name": "plex-issues",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start-api\" \"npm run start-ui\" ",
    "build": "ng build",
    "start:dev": "nodemon",
    "start-api": "npx nodemon",
    "start-ui": "npx ng serve --proxy-config proxy.conf.json",
    "watch:client": "ng serve --proxy-config proxy.conf.json",
    "watch:server": "rimraf ./bin && webpack --w --colors"
  },
  "bin": "server/server.js",
  "pkg": {
    "assets": [
      "dist/**/*",
      "server/models"
    ],
    "scripts": [
      "server/server.js"
    ],
    "targets": [
      "node8-win-x64"
    ]
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/typeorm": "^5.2.2",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "rxjs": "^6.3.3",
    "sql.js": "^0.5.0",
    "typeorm": "^0.2.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.0.0",
    "@angular/cli": "^7.0.1",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/material": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@types/node": "^10.12.0",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.0.1",
    "core-js": "^2.5.7",
    "ngx-webstorage": "^2.0.1",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "zone.js": "^0.8.26"
  }
}
