{
  "name": "plex-issues",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start-api\" \"npm run start-ui\" ",
    "build": "ng build",
    "start-api": "npx nodemon server/server.js",
    "start-ui": "npx ng serve --proxy-config proxy.conf.json",
    "test": "snyk test && ng test --watch false && npm run api-test && npm run int-test",
    "api-test": "node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/mocha server/tests/unit/*.test.js",
    "int-test": "node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/mocha server/tests/integration/*.test.js"
  },
  "bin": "server/server.js",
  "pkg": {
    "assets": [
      "dist/**/*",
      "server/models"
    ],
    "scripts": [
      "server/server.js"
    ],
    "targets": [
      "node8-win-x64"
    ]
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "morgan": "^1.8.2",
    "sequelize": "^4.4.2",
    "sqlite3": "^3.1.9"
  },
  "devDependencies": {
    "@angular/animations": "^4.3.3",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/cli": "^1.2.7",
    "@angular/common": "^4.3.3",
    "@angular/compiler": "^4.3.3",
    "@angular/compiler-cli": "^4.3.3",
    "@angular/core": "^4.3.3",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.3.3",
    "@angular/http": "^4.3.3",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.3.3",
    "@angular/platform-browser-dynamic": "^4.3.3",
    "@angular/router": "^4.3.3",
    "@ngtools/webpack": "^1.5.5",
    "@types/jasmine": "2.5.53",
    "@types/node": "~8.0.20",
    "codelyzer": "~3.1.2",
    "concurrently": "^3.5.0",
    "core-js": "^2.5.0",
    "cross-env": "^5.0.5",
    "expect.js": "^0.3.1",
    "jasmine": "^2.7.0",
    "jasmine-core": "^2.7.0",
    "jasmine-spec-reporter": "^4.2.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^3.5.0",
    "ngx-pagination": "^3.0.1",
    "ngx-webstorage": "^1.8.0",
    "nodemon": "^1.11.0",
    "pkg": "^4.2.2",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "shelljs": "^0.7.8",
    "snyk": "^1.38.1",
    "supertest": "^3.0.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.6.0",
    "typescript": "~2.4.2",
    "version-bump": "^0.1.0",
    "zone.js": "^0.8.16"
  }
}
