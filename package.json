{
  "name": "plex-issues",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start-api\" \"npm run start-ui\" ",
    "build": "ng build",
    "start-api": "npx nodemon server/server.js",
    "start-ui": "npx ng serve --proxy-config proxy.conf.json",
    "test": "snyk test && ng test --watch false && npm run api-test && npm run int-test",
    "api-test": "node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/mocha server/tests/unit/*.test.js",
    "int-test": "node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/mocha server/tests/integration/*.test.js"
  },
  "bin": "server/server.js",
  "pkg": {
    "assets": [
      "dist/**/*",
      "server/models"
    ],
    "scripts": [
      "server/server.js"
    ],
    "targets": [
      "node8-win-x64"
    ]
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "joi": "^13.1.2",
    "lokijs": "^1.5.2",
    "morgan": "^1.9.0",
    "nosql": "^6.1.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.33.4",
    "sqlite3": "^3.1.13",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/cli": "^1.6.8",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/flex-layout": "^2.0.0-rc.1",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@ngtools/webpack": "^1.9.8",
    "@types/jasmine": "2.6.0",
    "@types/node": "^8.0.58",
    "codelyzer": "^3.2.2",
    "concurrently": "^3.5.1",
    "core-js": "^2.5.3",
    "cross-env": "^5.1.3",
    "expect.js": "^0.3.1",
    "jasmine": "^2.99.0",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^3.5.3",
    "ngx-pagination": "^3.0.3",
    "ngx-webstorage": "^1.8.0",
    "nodemon": "^1.14.12",
    "pkg": "^4.3.0",
    "prettier": "^1.10.2",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "shelljs": "^0.7.8",
    "snyk": "^1.69.7",
    "supertest": "^3.0.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.5.3",
    "version-bump": "^0.1.0",
    "zone.js": "^0.8.20"
  }
}
